include /manual.pug

+page("HTTP Module")
	+manual()
		include common/header.pug

		+manual-body()
			+manual-toc()
				include:markdown http/toc.md

			+manual-content()
				+manual-chapter("overview", "HTTP Module")
					include:markdown http/overview.md

				+manual-chapter("url", "URL")
					include:markdown http/url.md

				+manual-chapter("server", "Server")
					include:markdown http/server.md

				+manual-chapter("router", "Router")
					include:markdown http/router.md

				+manual-chapter("request", "Request")
					include:markdown http/request.md

				+manual-chapter("response", "Response")
					include:markdown http/response.md

		include common/footer.pug
