var classMembers = members(Array(function() {}).getClass());
classMembers.sort();

print("**Member Functions**");
print();
print("| Function | Description |");
print("| ------ | ----------- |");

for member in classMembers {
	if member.startsWith("__") {
		member = "\\_\\_" + member.right(member.len() - 2);
	}

	print("| ", member, "() | |");
}
