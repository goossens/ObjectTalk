function render(canvas) {
	canvas.beginPath();
	canvas.drawRoundedRect(100, 100, 200, 200, 10);
	canvas.fillColor(0.0, 0.8, 1.0, 0.8);
	canvas.fill();
	canvas.strokeColor(0.0, 0.0, 0.0, 0.8);
	canvas.strokeWidth(15);
	canvas.stroke();

	canvas.beginPath();
	canvas.moveTo(150, 150);
	canvas.lineTo(250, 150);
	canvas.lineTo(200, 200);
	canvas.closePath();
	canvas.fillColor(0.5, 0.0, 0.8, 1.0);
	canvas.fill();
	canvas.strokeColor(0.0, 0.0, 0.0, 1.0);
	canvas.strokeWidth(15);
	canvas.stroke();
}
