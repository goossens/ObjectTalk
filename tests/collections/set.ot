var set = Set(1, 2, 3, 4);
assert(set == Set(1, 2, 3, 4));
assert(set.size() == 4);

set.insert(5);
assert(set == Set(1, 2, 3, 4, 5));
assert(set.size() == 5);

set.erase(4);
assert(set == Set(1, 2, 3, 5));
assert(set.size() == 4);

set += 8;
assert(set == Set(1, 2, 3, 5, 8));
assert(set.size() == 5);

set -= 8;
assert(set == Set(1, 2, 3, 5));
assert(set.size() == 4);

var total = 0;

for number in set {
	total += number;
}

assert(total == 11);

assert(set.contains(2));
assert(!set.contains(4));
assert(set.contains(5));

assert(2 in set);
assert(4 not in set);
assert(5 in set);

var set2 = set.clone();
assert(set2 == Set(1, 2, 3, 5));
assert(set2.size() == 4);
assert(set == set2);

set2.clear();
assert(set2 == Set());
assert(set2.size() == 0);
assert(set != set2);

set2 = Set(1, 3, 6, 8);
assert(set.intersection(set2) == Set(1, 3));
assert(set.difference(set2) == Set(2, 5, 6, 8));
assert(set.union(set2) == Set(1, 2, 3, 5, 6, 8));
assert(set.subtract(set2) == Set(2, 5));
