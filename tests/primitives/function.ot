// Function class test

function lower(s) {
	return s.lower();
}

var upper = function(s) {
	return s.upper();
};

function test(s, f) {
	return f(s);
}

assert(test("Hello", lower) == "hello");
assert(test("Hello", upper) == "HELLO");

var trim = function(s) {
	return s.trim();
};

assert(trim(" test	") == "test");

var global = 1;

assert(test(true, function(bool) {
	return bool == true && global == 1;
}) == true);
