var a = 25;
var b = 30;

{
	assert(a == 25);
	assert(b == 30);

	a *= 2;
	assert(a == 50);
	assert(b == 30);

	b /= 2;
	assert(a == 50);
	assert(b == 15);

	var a = b * b;
	assert(a == 225);
	assert(b == 15);
 }

 assert(a == 50);
 assert(b == 15);
